<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="70.0" prefWidth="200.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TextField fx:id="usernameField" promptText="Username" />
      <PasswordField fx:id="passwordField" promptText="Password" />
      <Button fx:id="signInButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Sign In" VBox.vgrow="ALWAYS" />
      <Button fx:id="backButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Back" />
   </children>
</fx:root>
